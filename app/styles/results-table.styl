.results-table
  width 100%
  max-width 640px
  margin auto
  border-collapse collapse

.results-table th
.results-table td
  text-align center
  padding 4px 0

.results-table tr
  border-bottom 1px solid dark-gray

.correct
  background-color #57d7af

.failure
  background-color #c30071
  color white
